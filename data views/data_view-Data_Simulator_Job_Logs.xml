			<attributes>
				<path><![CDATA[_calc:progress_status::string:"var x = get(\"Progress\");
x;":]]></path>
			</attributes>
		</column>
		<column path="sys_user_id" show_in_table="t"/>
		<column path="queue_time" show_in_table="t"/>
